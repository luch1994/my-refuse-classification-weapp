/* pages/about/detail.wxss */

.container {
  width: 100%;
  padding: 40rpx;
}

.item {
  position: relative;
  margin: 20rpx 0;
  padding-left: 30rpx;
  line-height: 48rpx;
  font-size: 32rpx;
}

.item::before {
  content: '';
  display: inline-block;
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background-color: #333;
  left: -10rpx;
  top: 14rpx;
}
